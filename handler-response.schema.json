{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://image.a11y.mcgill.ca/handler-response.schema.json",
    "type": "object",
    "title": "Handler Response",
    "description": "Rendering information generated by a handler.",
    "properties": {
        "request_uuid": { "$ref": "definitions.json#/definitions/request_uuid" },
        "timestamp": { "$ref": "definitions.json#/definitions/timestamp" },
        "renderings": {
            "description": "Renderings produced by the handler.",
            "type": "array",
            "items": { "$ref": "definitions.json#/definitions/rendering" }
        }
    },
    "required": [ "request_uuid", "timestamp", "renderings" ]
}
