{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://image.a11y.mcgill.ca/preprocessors/celebrity-detector.schema.json",
    "type": "object",
    "title": "Clothes Detector",
    "description": "Preprocessor to detect the clothes worn by the individuals. The associated information can be found in preprocessor named 'ca.mcgill.a11y.image.preprocessor.clothesDetector'. ",
    "properties":{
        "celebrity":{
            "type":"array",
            "description": "Contains a list of clothes worn by people",
            "items": {
                "type": "object",
                "properties": {
                    "ID":{
                        "description": "The ID of a person. The ID is generated by object detection preprocessor and is reused in this preprocessor",
                        "type": "integer"
                    },
                    "clothes":{
                        "type":"array",
                        "description": "Name of the detected celebrity.",
                        "items": {
                            "type": "object",
                            "description": "List of outfits worn by the individual",
                            "properties": {
                                "outfit":{
                                    "type":"string",
                                    "description": "One of the outfit worn by the individual"
                                },
                                "confidence":{
                                    "type": "number",
                                    "description": "confidence value for the detected outfit"
                                }
                            },
                            "required": ["outfit","confidence"]
                        }
                    }
                },
                "required": [
                    "ID",
                    "celebrity_name"
                  ]
            }
        }
    }
}
