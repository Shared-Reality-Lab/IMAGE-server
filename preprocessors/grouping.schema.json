{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://bach.cim.mcgill.ca/atp/preprocessors/grouping.schema.json",
    "type": "object",
    "title": "Grouping Data",
    "description": "Grouped data for objects",
    "properties": {
        "grouped": {
            "description": "An array of indices of objects having same type",
            "type": "array",
            "items": {
                "type": "array",
                "items": {
                    "type": "number"
                }
            }
        },
        "ungrouped": {
            "description": "An array of indices of objects which are unique and are not repeated in image",
            "type": "array",
            "items": {
                "type": "array",
                "items": {
                    "type": "number"
                }
            }
        }
    },
    "required": ["grouped", "ungrouped"]
}
