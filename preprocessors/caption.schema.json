{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://image.a11y.mcgill.ca/preprocessors/caption-generation.schema.json",
    "type": "object",
    "title": "Caption generator",
    "description": "Caption generated by ca.mcgill.a11y.image.preprocessor.captionGeneration",
    "properties": {
        "caption": {
            "description": "A single sentence caption that is generated for the input graphic",
            "type": "string",
            "minLength": 5     
        }  
    },
    "required": ["caption"]
}
