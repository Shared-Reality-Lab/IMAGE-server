{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "https://image.a11y.mcgill.ca/preprocessor-response.schema.json",
    "type": "object",
    "title": "Preprocessor Response",
    "description": "Preprocessor information generated by a preprocessor node.",
    "properties": {
        "request_uuid": { "$ref": "definitions.json#/definitions/request_uuid" },
        "timestamp": { "$ref": "definitions.json#/definitions/timestamp" },
        "name": {
            "description": "The unique reverse domain name identifier of the preprocessor that the data will be available under.",
            "type": "string",
            "pattern": "^[A-Za-z]{2,6}(\\.((?!-)[A-Za-z0-9-]+(?<!-)))+$"
        },
        "data": {
            "description": "The well-defined data produced by the preprocessor.",
            "type": "object",
            "properties": {
                "data_schema": { "$ref": "definitions.json#/definitions/data_schema" }
            }
        }
    },
    "required": [
        "request_uuid",
        "timestamp",
        "name",
        "data"
    ]
}
